
import time

def count_ways_to_summit(n):
    '''
    Docstring for count_ways_to_summit
    
    :param n: Total number of steps to reach the top
    '''

    prev = 0
    current = 0

    for _ in range(1, n + 1):

        temp = prev
        prev = current

        if prev == 0:
            current += 1
            prev = 1
        else:
            current += temp

    return current

start = time.time()
print("Number of ways of climbing mountain:",count_ways_to_summit(5))
end = time.time()
print(f"Time: {end - start} sec")